{
  "name": "markdeck",
  "version": "0.1.1",
  "description": "Generate slide decks from markdown files from within the command line.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node server.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djgrant/markdeck.git"
  },
  "keywords": [
    "markdown",
    "slides",
    "slide deck",
    "command line"
  ],
  "author": "Daniel Grant <hello@danielgrant.im> (http://github.com/djgrant)",
  "license": "MIT",
  "preferGlobal": true,
  "bugs": {
    "url": "https://github.com/djgrant/markdeck/issues"
  },
  "bin": {
    "markdeck": "server.js"
  },
  "homepage": "https://github.com/djgrant/markdeck",
  "dependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.13.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "express-handlebars": "^3.0.0",
    "marked": "^0.3.5",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-markdown": "^1.1.1",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.3"
  }
}
